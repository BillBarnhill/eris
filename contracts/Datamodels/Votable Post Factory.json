{
  "actions": {
    "upvotePost": {
      "precall": [
          "$this:0x19",
          "$doug:BLWVoteUpDown"
        ],
      "call": [
          "$precall[1]",
          "upvote",
          "$this"
        ],
      "postcall": [
          "$this:0x19"
        ],
      "success": "$postcall[0] > $precall[0]",
      "result":"$postcall[0]"
    },

    "downvotePost": {
      "precall": [
          "$this:0x20",
          "$doug:BLWVoteUpDown"
        ],
      "call": [
          "$precall[1]",
          "downvote",
          "$this"
        ],
      "postcall": [
          "$this:0x20"
        ],
      "success": "$postcall[0] > $precall[0]",
      "result": "$postcall[0]"
    }
  },

  "data": {
    "upvotes":"$this:0x19",
    "downvotes":"$this:0x20"
  }
}