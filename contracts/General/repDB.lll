			}
		)

		(when (= (cmd) "addstrike")
			{
				(unless (CHECKBYLAW) (STOP))
				[0x0](calldataload 0x20)
				(unless @0x0 [0x0]1) ;Default 1 strike
				[[(+ (target) 1)]](+ @@(+ (target) 1) @0x0)
				(STOP)
			}
		)

		(when (= (cmd) "rmstrike")
			{
				(unless (CHECKBYLAW) (STOP))
				[[(+ (target) 1)]](- @@(+ (target) 1) 1)
				(STOP)
			}
		)

		(when (= (cmd) "makemem")
			{
				(unless (CHECKBYLAW) (STOP))
				[[(+ (target) 2)]]1
