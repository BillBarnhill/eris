	(return 0 (lll{
		;Body
		(when (= (cmd) "init")
			{
				(unless @@0x20 (STOP))
				[[(caller)]]0x900001
				[[(+ (caller) 2)]] 1
				[[0x20]]1
			}
		)

		(when (= (cmd) "get")
			{
				[0x0]@@(target)
